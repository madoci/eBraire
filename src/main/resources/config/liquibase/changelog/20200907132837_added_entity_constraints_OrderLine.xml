<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity OrderLine.
    -->
    <changeSet id="20200907132837-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="order_lines_id"
                                 baseTableName="order_line"
                                 constraintName="fk_order_line_order_lines_id"
                                 referencedColumnNames="id"
                                 referencedTableName="book"/>

        <addForeignKeyConstraint baseColumnNames="order_id"
                                 baseTableName="order_line"
                                 constraintName="fk_order_line_order_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ordered"/>

    </changeSet>
</databaseChangeLog>
